<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joinservice.joinservice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TelaLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TelaPrincipal" />
        <activity android:name=".register.RegisterEmailActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".register.RegisterProfileActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".register.RegisterPictureActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".register.RegisterPhoneActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".principal.consumer.ListOrderConsumerActivity"
            android:label="@string/title_activity_list_order_consumer"
            android:theme="@style/AppTheme" />
        <activity android:name=".EditProfile"
            android:screenOrientation="portrait"/>
    </application>

</manifest>