<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".principal.consumer.StartCliente">

    <ListView
        android:id="@+id/lvSolicitacoesServicos"
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:layout_marginTop="48dp"
        android:divider="@color/colorDestaque"></ListView>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/btCadastrarServico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        app:backgroundTint="@color/colorAzulMarinho"
        app:fabSize="normal"
        app:rippleColor="@color/colorAccent"
        app:srcCompat="@android:drawable/ic_input_add" />

    <RelativeLayout
        android:id="@+id/relativeLayoutContainerteste"
        android:layout_width="292dp"
        android:layout_height="600dp"
        android:layout_alignParentRight="true"
        android:layout_marginTop="48dp"
        ></RelativeLayout>


</RelativeLayout>